    "type": "object",
    "additionalProperties": false,
    "properties": {
      "id": {
        "type": "integer",
        "format": "int64"
      },
      "nome": {
        "type": "string"
      },
      "localizacao": {
        "type": "string"
      },
      "tipoCozinha": {
        "type": "string"
      },
      "horarioFuncionamento": {
        "type": "string"
      },
      "capacidade": {
        "type": "integer",
        "format": "int32"
      }
    },
    "required": [
      "nome",
      "localizacao",
      "tipoCozinha",
      "horarioFuncionamento",
      "capacidade"
    ]
  }



  